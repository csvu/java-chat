<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ColorPicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<VBox alignment="CENTER" prefHeight="243.0" prefWidth="311.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1"
      xmlns="http://javafx.com/javafx/8" fx:controller="mop.app.client.controller.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <children>
        <ColorPicker>
            <opaqueInsets>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
            </opaqueInsets>
        </ColorPicker>
        <CheckBox mnemonicParsing="false" text="CheckBox"/>

        <Label fx:id="welcomeText"/>
        <Button onAction="#onHelloButtonClick" text="Hello!"/>
    </children>
</VBox>
