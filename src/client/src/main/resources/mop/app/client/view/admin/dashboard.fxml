<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="700.0" prefWidth="1000.0" styleClass="container" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mop.app.client.controller.admin.DashboardController">
    <stylesheets>
        <URL value="@../../css/admin.css" />
    </stylesheets>
    <padding>
        <Insets left="40.0" right="20.0" />
    </padding>
   <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Hi, Admin" AnchorPane.leftAnchor="20.0" AnchorPane.topAnchor="30.0" />
   <VBox prefHeight="200.0" prefWidth="100.0" />
   <VBox AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="300.0">
      <children>
          <BarChart fx:id="barChart" prefHeight="400" prefWidth="500">
              <xAxis>
                  <CategoryAxis fx:id="monthAxis" label="Month" />
              </xAxis>
              <yAxis>
                  <NumberAxis fx:id="userAxis" label="New Registrations" />
              </yAxis>
          </BarChart>
      </children>
   </VBox>
</AnchorPane>
