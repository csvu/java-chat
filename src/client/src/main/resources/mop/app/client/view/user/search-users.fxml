<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<fx:root fx:id="searchUsers" alignment="CENTER_LEFT" prefHeight="65.0" spacing="10.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ImageView fitHeight="24.0" fitWidth="24.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@search-alt-2-svgrepo-com.png" />
         </image>
      </ImageView>
      <TextField fx:id="searchUsersTextField" promptText="Search">
         <font>
            <Font size="10.0" />
         </font></TextField>
      <ImageView fx:id="cancel" fitHeight="24.0" fitWidth="24.0" layoutX="30.0" layoutY="31.0" pickOnBounds="true" preserveRatio="true" style="-fx-opacity: 0.8;">
         <image>
            <Image url="@cancel1-svgrepo-com.png" />
         </image>
      </ImageView>
   </children>
   <padding>
      <Insets bottom="10.0" left="20.0" right="10.0" top="10.0" />
   </padding>
</fx:root>
